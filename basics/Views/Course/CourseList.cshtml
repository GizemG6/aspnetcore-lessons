@Model List<Course>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>course list</h1>

    @foreach(var course in Model)
    {
        <div id="course_@course.Id">
            <h2>@course.Title</h2>
            <p>@course.Description</p>
            <img src="~/img/@course.Image" width="200" alt="">
        </div>
    }

</body>
</html>